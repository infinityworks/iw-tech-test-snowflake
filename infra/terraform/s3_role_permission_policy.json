{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
              "s3:GetObject",
              "s3:GetObjectVersion"
            ],
            "Resource": "arn:aws:s3:::babak4-test-convex/data/*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::babak4-test-convex",
            "Condition": {
                "StringLike": {
                    "s3:prefix": [
                        "data/*"
                    ]
                }
            }
        }
    ]
}